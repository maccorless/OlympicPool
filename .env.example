# Flask Configuration
FLASK_SECRET_KEY=dev-secret-key-for-local-testing-only
BASE_URL=http://localhost:5001

# Admin Emails (comma-separated)
ADMIN_EMAILS=admin@example.com

# Optional - Email Service (leave blank for console output in dev)
RESEND_API_KEY=

# Dev mode: show magic links on page instead of sending email (set to False in production)
NO_EMAIL_MODE=True

# Optional - Session Security (set to true in production)
SESSION_COOKIE_SECURE=False
